//AVL Trees

class IntAVLNode extends IntBSTNode{
	
	protected int balanceFactor,key;
    protected IntAVLNode left, right, parent;
    
    public IntAVLNode() {
        left = right = parent = null;
    }
    
    public IntAVLNode(int el, IntAVLNode par) {
        this(el,null,null,par);
    }
    
    public IntAVLNode(int el, IntAVLNode lt, IntAVLNode rt, IntAVLNode par) {
        key = el; 
       	left = lt; 
        right = rt; 
        parent = par;
    }
    
    public String toString() {
        return ("" + key);
    }
	
}//end - class

	
	
